            </attenuation>
            <direction>-0.5 0.1 -0.9</direction>
        </light>
        
        
        
          <model name="custom_ground_plane">
    <static>true</static>
    <pose>0 0 0 0 0 0</pose>
    <link name="ground_link">
      <!-- Collision geometry -->
      <collision name="collision">
        <geometry>
          <plane>
            <normal>0 0 1</normal>
            <size>10000 10000</size> <!-- Width x Depth in meters -->
          </plane>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100</mu>
              <mu2>100</mu2>
            </ode>
          </friction>
<contact>
  <ode>
    <kp>100000</kp>  <!-- Contact stiffness -->
    <kd>100</kd>     <!-- Contact damping -->
    <min_depth>0.001</min_depth>
  </ode>
</contact>
        </surface>
      </collision>

      <!-- Visual geometry -->
      <visual name="visual">
        <geometry>
          <plane>
            <normal>0 0 1</normal>
            <size>10000 10000</size>
          </plane>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1</ambient>
          <diffuse>0.2 0.2 0.2 1</diffuse>  <!-- Dark grey -->
          <specular>0.05 0.05 0.05 1</specular>
          <emissive>0 0 0 1</emissive>
        </material>
      </visual>
    </link>
  </model>


   
