<model name="custom_box">
    <static>false</static> <!-- Set to true if you want it fixed -->
    <pose>-780 400 50 0 0 0</pose> <!-- Height offset = half the box height -->
    <link name="box_link">
      <!-- Collision -->
      <collision name="box_collision">
        <geometry>
          <box>
            <size>20 20 20</size> <!-- Length x Width x Height (m) -->
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.8</mu>
              <mu2>0.8</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <!-- Visual -->
      <visual name="box_visual">
        <geometry>
          <box>
            <size>20 20 20</size>
          </box>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.2 1</ambient>
          <diffuse>0.4 0.4 0.8 1</diffuse> <!-- Blueish color -->
          <specular>0.1 0.1 0.2 1</specular>
        </material>
      </visual>

      <!-- Inertial (optional, helps physics stability) -->
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>1.0</ixx>
          <iyy>1.0</iyy>
          <izz>1.0</izz>
        </inertia>
      </inertial>
    </link>
  </model>



